{
  "name": "webhooks-client",
  "version": "0.0.0",
  "description": "",
  "main": "main.js",
  "browserify": {
    "transform": [
      "babelify",
      [
        "localenvify",
        {
          "envfile": "./local.env"
        }
      ]
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "cyclic-router": "^2.0.0",
    "fwitch": "^1.0.1",
    "most": "^0.19.6",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "11.0.1",
    "browserify-incremental": "^3.0.1",
    "instant-server": "^2.0.0",
    "localenvify": "^1.0.1"
  },
  "scripts": {
    "style": "stylus src/style.styl -o dist/style.css",
    "browserify": "mkdirp dist && browserify src/main.js --outfile dist/main.js",
    "live": "find src/*.js -type f | grep -v 'swp' | entr fish -c 'browserifyinc src/main.js --outfile dist/main.js; and echo \"done!\"'",
    "server": "instant -p 5656",
    "build-prod": "NODE_PRODUCTION=production browserify src/main.js --outfile dist/main.js",
    "deploy": "npm run build-prod && cp index.html dist/ && surge dist webhooks"
  },
  "author": "",
  "license": "ISC"
}
